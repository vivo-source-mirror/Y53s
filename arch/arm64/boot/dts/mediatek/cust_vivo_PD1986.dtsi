/* fuxi add for battery&charge start */
&pio {
	battery_id_pins_default: bat_adc_default {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO28__FUNC_GPIO28>;
			slew-rate = <1>;
			output-high;
		};
	};

	battery_id_pins_reset: bat_adc_reset {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO28__FUNC_GPIO28>;
			slew-rate = <0>;
			bias-disable;
			output-low;
		};
	};

	battery_id_pins_output: bat_adc_output {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO28__FUNC_GPIO28>;
			slew-rate = <1>;
			output-high;
		};
	};
};


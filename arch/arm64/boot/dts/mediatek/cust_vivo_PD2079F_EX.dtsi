	};

	card_detect: card_sim_detect {
		pins_cmd0_dat {
			pinmux = <PINMUX_GPIO140__FUNC_MD_INT1_C2K_UIM0_HOT_PLUG>;
			slew-rate = <0>;
			bias-disable;
		};
		pins_cmd1_dat {
			pinmux = <PINMUX_GPIO141__FUNC_GPIO141>;
			slew-rate = <0>;
			bias-disable;
		};
		//pins_cmd2_dat {
		//	pinmux = <PINMUX_GPIO18__FUNC_GPIO18>;
		//	slew-rate = <0>;
		//	bias-disable;
		//};
